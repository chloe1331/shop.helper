{
    "presets": [
        "es2015",
        "react",
        "stage-0"
    ],
    "plugins": [
        [
            "babel-plugin-root-import", [{
                "rootPathPrefix": "~",
                "rootPathSuffix": "src"
            }, {
                "rootPathPrefix": "-",
                "rootPathSuffix": "server"
            }]
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": true,
                "css": true
            }
        ]
    ]
}